{
	"Form Component": {
	  "prefix": "frm",
	  "body": [
		"import React, { useEffect, useState } from 'react';",
		"",
		"function ComponentName() {",
		"    const [newValueName1, setNewValueName1] = useState('');",
		"    const [newValueName2, setNewValueName2] = useState('');",
		"    const [newValueName3, setNewValueName3] = useState('');",
		"    const [newValueName4, setNewValueName4] = useState('');",
		"    const [newValueName5, setNewValueName5] = useState('');",
		"",
		"    const [newData, setNewData] = useState(() => {",
		"        const savedData = localStorage.getItem('newData');",
		"        return savedData ? JSON.parse(savedData) : [];",
		"    });",
		"",
		"    useEffect(() => {",
		"        localStorage.setItem('newData', JSON.stringify(newData));",
		"    }, [newData]);",
		"",
		"    const handleSubmit = () => {",
		"        if (newValueName1 && newValueName2 && newValueName3) {",
		"            const newReturn = {",
		"                new_val1: newValueName1,",
		"                new_val2: newValueName2,",
		"                new_val3: newValueName3,",
		"                new_val4: newValueName4,",
		"                new_val5: newValueName5,",
		"            };",
		"            setNewData([...newData, newReturn]);",
		"            setNewValueName1('');",
		"            setNewValueName2('');",
		"            setNewValueName3('');",
		"            setNewValueName4('');",
		"            setNewValueName5('');",
		"        }",
		"    };",
		"",
		"    return (",
		"        <div className=''>",
		"            <p>Submit your data information here</p>",
		"            <form>",
		"                <div className='input-areas'>",
		"                    <input ",
		"                        type='text'",
		"                        id='name1'",
		"                        placeholder='Enter Value 1'",
		"                        value={newValueName1}",
		"                        onChange={(e) => setNewValueName1(e.target.value)}",
		"                        required",
		"                    />",
		"                    <input ",
		"                        type='text'",
		"                        id='name2'",
		"                        placeholder='Enter Value 2'",
		"                        value={newValueName2}",
		"                        onChange={(e) => setNewValueName2(e.target.value)}",
		"                        required",
		"                    />",
		"                    <input ",
		"                        type='text'",
		"                        id='name3'",
		"                        placeholder='Enter Value 3'",
		"                        value={newValueName3}",
		"                        onChange={(e) => setNewValueName3(e.target.value)}",
		"                        required",
		"                    />",
		"                    <input ",
		"                        type='text'",
		"                        id='name4'",
		"                        placeholder='Enter Value 4'",
		"                        value={newValueName4}",
		"                        onChange={(e) => setNewValueName4(e.target.value)}",
		"                    />",
		"                    <input ",
		"                        type='text'",
		"                        id='name5'",
		"                        placeholder='Enter Value 5'",
		"                        value={newValueName5}",
		"                        onChange={(e) => setNewValueName5(e.target.value)}",
		"                    />",
		"                </div>",
		"            </form>",
		"            <button className='submit-btn' onClick={handleSubmit}>Submit</button>",
		"        </div>",
		"    );",
		"};",
		"",
		"export default ComponentName;"
	  ],
	  "description": "A reusable Form component with local storage integration."
	},
}
